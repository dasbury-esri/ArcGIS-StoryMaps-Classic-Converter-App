        "n-dZVTRQ": {
            "type": "text",
            "data": {
                "type": "paragraph",
                "text": "<strong>Four-year colleges and universities are just part of a rich ecosystem of post-secondary schools.</strong>"
            },
            "config": {
                "size": "wide"
            }
        },
        "n-jF8ihJ": {
            "type": "text",
            "data": {
                "type": "paragraph",
                "text": "This map shows the most common type of post-secondary institution by area. Bigger icons represent clusters of schools. As you pan and zoom, you will see more granular views of the area. Click an icon for more information."
            },
            "config": {
                "size": "wide"
            }
        },
        "n-tcBhyB": {
            "type": "image",
            "data": {
                "image": "r-p8CA84"
