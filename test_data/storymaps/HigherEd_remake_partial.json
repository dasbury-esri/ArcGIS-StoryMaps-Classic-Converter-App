        "n-llpsRH": {
            "type": "image",
            "data": {
                "image": "r-3fgwl5",
                "alt": "This chart shows the main types of post-secondary institutions in the United States and a count of their programs. Universities and professional schools account for over a third of all institutions, community colleges account for a quarter. The remaining school types including cosmetology and barber school, technical and trade schools, and other schools.",
                "isExpandable": false
            },
            "config": {
                "size": "wide"
            }
        },
        "n-7smYTX": {
            "type": "text",
            "data": {
                "type": "paragraph",
                "text": "While four-year universities are the most common type of institution, community colleges make up fully a quarter of schools nationally."
            },
            "config": {
                "size": "wide"
            }
        },
        "n-vf4za0": {
            "type": "text",
            "data": {
                "type": "paragraph",
                "text": "Patterns of schools vary geographically. Explore the educational landscape of your region below."
